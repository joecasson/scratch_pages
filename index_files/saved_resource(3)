    var changeDefault = function () {
      var addHtml;

      if(!$('#minicart-items').length){
        return;
      };

      //var priceWithoutDelivery = 0;
      var priceOfAllItems = 0;
      var shippingPrice;
      var priceForFreeDelivery;
      var discount = 0;

      var subTotal;

      jQuery('dd.promo').each(function(){
          discount += parseInt(jQuery(this).text().replace(/[^0-9]/g, ''));
      });

      jQuery('.cart-item-price .price').each(function(){
        priceOfAllItems += parseInt(jQuery(this).text().replace(/[^0-9]/g, ''));
      });

      subTotal = priceOfAllItems - discount;
      
      $('dd.total, #minicart-hover span.total').text('£' + (subTotal/100).toFixed(2));

      if (subTotal >= 5000){
        shippingPrice = 0;
        priceForFreeDelivery = 0;
        addHtml = mmHtml.find('.mt10-wrap-two').html();
        $('#minicart-footer').append(addHtml);
      } else {
        shippingPrice = $('dd.shipping').text().replace(/[^0-9]/g, '');
        //priceForFreeDelivery = 5000 - priceOfAllItems;
        priceForFreeDelivery = 5000 - subTotal;
        addHtml = mmHtml.find('.mt10-wrap-one').html();
        $('#minicart-footer').append(addHtml);
        $('.mt10-price-bl').text('£' + (priceForFreeDelivery/100).toFixed(2));
      }
    };

    changeDefault();

