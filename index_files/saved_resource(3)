(function($){
  'use strict';
  (function waiter (){
    var timeOut;

    setTimeout(function(){
      if(timeOut){
        clearTimeout(timeOut);
      }
    }, 3000);
    
    if ($('#minicart-drop li form .grey.gradient').size() > 0 && $('#minicart-checkout').size() > 0){
      
      $('#minicart-drop li form .grey.gradient').click(function(){
        mmcore.SetCookie('mm_checkout_click', 'minibasket', 5 / (24 * 60 * 60), 1);
      });

      $('#minicart-checkout').click(function(){
        mmcore.SetCookie('mm_checkout_click', 'header', 5 / (24 * 60 * 60), 1);
      });

    } else {
      timeOut = setTimeout(waiter, 100);
    }

  }());
  
}(mmcore.$));
}catch(err){mmcore.EH(err);}
